// Diagrama do Circuito
digraph {
	dpi=300 rankdir=LR size="12,8"
	subgraph cluster_inputs {
		color=lightblue label=Entradas
		a [label="a
(Entrada)" fillcolor=lightblue shape=box style=filled]
		b [label="b
(Entrada)" fillcolor=lightblue shape=box style=filled]
		cin [label="cin
(Entrada)" fillcolor=lightblue shape=box style=filled]
	}
	subgraph cluster_outputs {
		color=lightgreen label="SaÃ­das"
		sum [label="sum
(Wire)" fillcolor=lightgreen shape=box style=filled]
		cout [label="cout
(Wire)" fillcolor=lightgreen shape=box style=filled]
	}
	nb [label="U1
(PortaNot)" shape=rectangle]
	a_and_nb [label="U2
(PortaAnd)" shape=rectangle]
	na [label="U3
(PortaNot)" shape=rectangle]
	na_and_b [label="U4
(PortaAnd)" shape=rectangle]
	x [label="U5
(PortaOr)" shape=rectangle]
	ncin [label="U6
(PortaNot)" shape=rectangle]
	x_and_ncin [label="U7
(PortaAnd)" shape=rectangle]
	nx [label="U8
(PortaNot)" shape=rectangle]
	nx_and_cin [label="U9
(PortaAnd)" shape=rectangle]
	a_and_b [label="U11
(PortaAnd)" shape=rectangle]
	cin_and_x [label="U12
(PortaAnd)" shape=rectangle]
	b -> nb
	a -> a_and_nb
	U1 -> a_and_nb
	a -> na
	U3 -> na_and_b
	b -> na_and_b
	U2 -> x
	U4 -> x
	cin -> ncin
	U5 -> x_and_ncin
	U6 -> x_and_ncin
	U5 -> nx
	U8 -> nx_and_cin
	cin -> nx_and_cin
	a -> a_and_b
	b -> a_and_b
	cin -> cin_and_x
	U5 -> cin_and_x
}
